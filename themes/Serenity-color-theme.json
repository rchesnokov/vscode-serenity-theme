{
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "comment.block",
        "punctuation.definition.comment",
        "punctuation.definition.comment.html"
      ],
      "settings": {
        "foreground": "#A0A3AA",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Text",
      "scope": "text",
      "settings": {}
    },
    {
      "name": "String",
      "scope": [
        "string",
        "string.quoted",
        "string.quoted.single",
        "string.quoted.double",
        "string.interpolated",
        "punctuation.definition.string",
        "punctuation.definition.entity"
      ],
      "settings": {
        "foreground": "#ebfff4"
      }
    },
    {
      "name": "HTML String",
      "scope": [
        "string.html",
        "string.quoted.html",
        "string.quoted.single.html",
        "string.quoted.double.html",
        "string.interpolated.html",
        "punctuation.definition.string.html"
      ],
      "settings": {
        "foreground": "#edfddb"
      }
    },
    {
      "name": "Tag",
      "scope": ["entity.name.tag", "meta.tag.sgml"],
      "settings": {
        "foreground": "#FEA044"
      }
    },
    {
      "name": "Keywords, Attributes",
      "scope": [
        "entity.other.attribute-name.js",
        "meta.attribute-with-value.id.html",
        "entity.other.attribute-name.id.html",
        "meta.attribute-with-value.class.html",
        "entity.other.attribute-name.class.html",
        "meta.attribute-with-value.html",
        "entity.other.attribute-name.html",
        "entity.name.tag.reference"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ffc830"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric", "constant.numeric.scss"],
      "settings": {
        "foreground": "#ff68ae"
      }
    },
    {
      "name": "Boolean",
      "scope": ["constant.language.boolean"],
      "settings": {
        "foreground": "#ff68ae"
      }
    },
    {
      "name": "Operator",
      "scope": [
        "keyword.operator",
        "keyword.operator.assignment",
        "keyword.operator.relational",
        "keyword.operator.arithmetic",
        "keyword.operator.compartion",
        "constant.other.color"
      ],
      "settings": {
        "foreground": "#ffa386"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation.other",
        "punctuation.definition",
        "punctuation.definition.block",
        "punctuation.separator",
        "punctuation.section.tag",
        "meta.delimiter",
        "meta.group.braces",
        "keyword.operator.accessor"
      ],
      "settings": {
        "foreground": "#888"
      }
    },
    {
      "name": "Constant, Parameter",
      "scope": [
        "constant.language",
        "support.constant",
        "constant.character",
        "meta.parameters",
        "punctuation.section.embedded",
        "entity.name.tag.css",
        "entity.name.tag.scss",
        "entity.name.tag.less"
      ],
      "settings": {
        "foreground": "#FEA044"
      }
    },
    {
      "name": "Css property value",
      "scope": ["support.constant.property-value", "constant.property-value"],
      "settings": {
        "foreground": "#F9F9F9"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword", "entity.name.tag.other"],
      "settings": {
        "foreground": "#ff86b9",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "Storage & Units",
      "scope": [
        "storage",
        "variable.language",
        "keyword.operator.new",
        "keyword.other.substitution",
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.from",
        "entity.other.attribute-name.pseudo-element",
        "entity.other.attribute-name.pseudo-class",
        "punctuation.definition.generic"
      ],
      "settings": {
        "foreground": "#7a70b3",
        "fontStyle": "italic bold",
      }
    },
    {
      "name": "Function",
      "scope": [
        "entity.name.function",
        "variable.function",
        "support.function",
        "keyword.other.special-method",
        "meta.function-call",
        "meta.block-level",
        "keyword.operator.typeof",
        "keyword.operator.module"
      ],
      "settings": {
        "foreground": "#B8A3E5"
      }
    },
    {
      "name": "Class",
      "scope": [
        "entity.name.class",
        "entity.name.type.class",
        "entity.name.type.instance",
        "entity.other.attribute-name",
        "support.type",
        "support.class",
        "support.orther.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter",
        "entity.other.attribute-name.generic"
      ],
      "settings": {
        "foreground": "#b1f6e8"
      }
    },
    {
      "name": "Class - css",
      "scope": [
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.parent-selector-suffix"
      ],
      "settings": {
        "foreground": "#CDFFE4"
      }
    },
    {
      "name": "Object",
      "scope": ["variable.other.object"], 
      "settings": {
        "foreground": "#cff2fd",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "Variable",
      "scope": [
        "variable",
        "variable.parameter",
        "variable.other.object.property",
        "variable.other.readwrite",
        "variable.other.constant.property",
        "object.key",
        "string.unquoted",
        "meta.arguments",
        "meta.property.object",
        "meta.jsx.js"
      ],
      "settings": {
        "foreground": "#eeeeee",
        "fontStyle": "normal"
      }
    },
    {
      "name": "Property",
      "scope": [
        "constant.other.object.key.js",
        "support.type.property-name",
        "meta.objectliteral.js",
        "meta.tag",
        "punctuation.definition.tag",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html"
      ],
      "settings": {
        "foreground": "#888",
        "fontStyle": "normal"
      }
    },
    {
      "name": "Object property",
      "scope": ["meta.object.member.js", "meta.object-literal.key.js"],
      "settings": {
        "foreground": "#e0ecff"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": [
        "string.regexp",
        "string.regexp keyword.operator",
        "constant.character.escape.backslash.regexp",
        "keyword.control.anchor.regexp",
        "keyword.other.unit",
        "constant.other.unit"
      ],
      "settings": {
        "foreground": "#bacfdb"
      }
    }
  ],
  "colors": {
    "editor.background": "#282d3a",
    "editor.foreground": "#E0E0E0",
    "editorCursor.foreground": "#ffc830",
    "editorWhitespace.foreground": "#33394e",
    "editor.lineHighlightBackground": "#333a52",
    "editor.selectionBackground": "#717885",
    "editorBracketMatch.border": "#ffc830",
    "activityBar.background": "#3f454e",
    "activityBar.foreground": "#999",
    "activityBarBadge.background": "#ffc830",
    "activityBarBadge.foreground": "#222222",
    "breadcrumb.background": "#282d3a",
    "breadcrumb.foreground": "#666",
    "breadcrumb.activeSelectionForeground": "#ffc830",
    "breadcrumbPicker.background": "#282d3a",
    "menu.foreground": "#666",
    "menu.selectionForeground": "#ffc830",
    "menu.selectionBackground": "#546086",
    "editorLineNumber.foreground": "#373c4d",
    "editorLineNumber.activeForeground": "#636b86",
    "sideBar.background": "#282d3a",
    "sideBar.foreground": "#999",
    "sideBarSectionHeader.background": "#3f454e",
    "sideBarSectionHeader.foreground": "#b9b9b9",
    "sideBarTitle.foreground": "#666",
    "list.activeSelectionBackground": "#545c69",
    "list.activeSelectionForeground": "#FEA044",
    "list.hoverBackground": "#45505e",
    "statusBar.background": "#545c69",
    "statusBar.foreground": "#ffc830",
    "statusBar.noFolderBackground": "#FEA044",
    "titleBar.activeBackground": "#3f454e",
    "titleBar.activeForeground": "#999999",
    "panel.background": "#282d3a",
    "editorGroupHeader.tabsBackground": "#282d3a",
    "editorGroupHeader.tabsBorder": "#424242",
    "tab.inactiveBackground": "#292e3d",
    "tab.inactiveForeground": "#666",
    "tab.activeBackground": "#3f454e",
    "tab.activeForeground": "#FEA044",
    "scrollbar.shadow": "#262b3b",
    "gitDecoration.modifiedResourceForeground": "#ffc525",
    "gitDecoration.untrackedResourceForeground": "#b3ff80"
  },
  "name": "Serenity"
}
