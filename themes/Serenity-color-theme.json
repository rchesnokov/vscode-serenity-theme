{
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"comment.block",
				"punctuation.definition.comment",
				"punctuation.definition.comment.html"
			],
			"settings": {
				"foreground": "#A0A3AA",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Text",
			"scope": "text",
			"settings": {}
		},
		{
			"name": "String",
			"scope": [
				"string",
				"string.quoted",
				"string.quoted.single",
				"string.quoted.double",
				"string.interpolated",
				"punctuation.definition.string",
				"punctuation.definition.entity"
			],
			"settings": {
				"foreground": "#e2fff5"
			}
		},
		{
			"name": "HTML String",
			"scope": [
				"string.html",
				"string.quoted.html",
				"string.quoted.single.html",
				"string.quoted.double.html",
